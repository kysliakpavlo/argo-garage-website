extends index 

block dashcontent    
  #leftcolumn 
    table
      tr
        th Key 
        th Tag
      tr
        td b 
        td Block
      tr
        td c 
        td Customer
      tr
        td p 
        td Porn
      tr
        td f 
        td Filter
      tr
        td n
        td News
      tr
        td r
        td Report
      tr
        td 1 
        td 90°
      tr
        td 2 
        td 180°
      tr
        td 3 
        td 270°
      tr
        td ' ' 
        td Done
  #middlecolumn
    #middletop
      p
        img(src=renderURL, max-width='100%', height='500px')
    #middleleftcolumn
      p 1
      p
        img(src=renderURL, max-width='33%', height='160px', class='rotate90')
    #middlemiddlecolumn
      p 2
      p
        img(src=renderURL, max-width='34%', height='160px', class='rotate180')
    #middlerightcolumn
      p 3
      p
        img(src=renderURL, max-width='33%', height='160px', class='rotate270')
    #middlebottom
      pre #{renderJSONdata}
  #rightcolumn     
    table
      tr
        th Attribute 
        th Value
      tr
        td Blocked 
        td #{renderBlocked}
      tr
        td Filtered 
        td #{renderFiltered}
      tr
        td Hidden 
        td #{renderHidden}
      tr
        td Orientation 
        td #{renderOrientation}
      